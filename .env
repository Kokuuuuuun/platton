# Pelican Panel Configuration
# Copy this file to .env and customize the values

# Application Configuration
APP_URL=https://panel.theoasiss.us
APP_TIMEZONE=America/New_York
# ADMIN_EMAIL not needed - Coolify handles SSL certificates

# Database Configuration (SQLite by default)
DB_CONNECTION=sqlite
# For MySQL/MariaDB, use:
# DB_CONNECTION=mysql
# DB_HOST=mysql
# DB_PORT=3306
# DB_DATABASE=pelican
# DB_USERNAME=pelican
# DB_PASSWORD=your_secure_password

# Cache Configuration
CACHE_DRIVER=file
QUEUE_CONNECTION=database
SESSION_DRIVER=file

# Wings Configuration
# You need to generate this token from the Panel after installation
WINGS_TOKEN=your_wings_token_here

# Note: Coolify handles SSL certificates automatically
# ADMIN_EMAIL is set to a default value for Caddyfile compatibility
